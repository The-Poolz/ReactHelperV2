{
  "name": "reacthelperv2",
  "version": "1.0.0",
  "type": "module",
  "source": "src/index.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.module.js",
    "require": "./dist/index.js",
    "default": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.module.js",
  "unpkg": "./dist/index.umd.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite example --config example/vite.config.ts",
    "clean": "rm -rf dist",
    "prebuild": "pnpm clean",
    "build": "pnpm prebuild && microbundle --jsx React.createElement && pnpm build:types",
    "build:types": "tsc --project tsconfig.build.json --emitDeclarationOnly",
    "test": "node --test",
    "contracts:update-types": "node scripts/generateContractTypes.js",
    "contracts:update": "node scripts/updatePoolzData.js"
  },
  "dependencies": {
    "@tanstack/react-query": "5.45.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "viem": "latest",
    "wagmi": "latest"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.6.0",
    "@wagmi/cli": "latest",
    "buffer": "^6.0.3",
    "microbundle": "^0.15.1",
    "typescript": "^5.8.3",
    "vite": "^5.4.19"
  }
}
